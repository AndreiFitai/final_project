{
  "name": "mern-boilerplate",
  "version": "1.0.0",
  "description": "Easy to use, mern boilerplate. Ready to deploy to heroku, no further configuration required.",
  "main": "index.js",
  "scripts": {
    "dev:app": "parcel watch src/app/index.html",
    "dev:server": "nodemon --watch src/server --max-old-space-size=4096 src/server/index.js",
    "dev": "concurrently -n app,server -c green,blue \"npm run dev:app\" \"npm run dev:server\"",
    "build": "parcel build src/app/index.html --out-dir src/server/public/app --public-url /app",
    "start": "NODE_ENV=production node src/server/index.js",
    "heroku-postbuild": "npm run build"
  },
  "author": "Lukas Gisder-Dub√©",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "base64url": "^3.0.0",
    "bcrypt": "^3.0.1",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "chart.js": "^2.7.2",
    "cloudinary": "^1.11.0",
    "compression": "^1.7.3",
    "cron": "^1.4.1",
    "express": "^4.16.3",
    "express-fileupload": "^0.4.0",
    "helmet": "^3.13.0",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "millify": "^2.0.1",
    "moment": "^2.22.2",
    "mongoose": "^5.2.17",
    "morgan": "^1.9.1",
    "node-telegram-bot-api": "^0.30.0",
    "qrcode": "^1.2.2",
    "react": "^16.5.2",
    "react-chartjs-2": "^2.7.4",
    "react-collapse": "^4.0.3",
    "react-dom": "^16.5.2",
    "react-motion": "^0.5.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^4.0.1",
    "cssnano": "^4.1.4",
    "node-sass": "^4.13.1",
    "parcel-bundler": "^1.10.1",
    "react-hot-loader": "^4.3.11"
  }
}
